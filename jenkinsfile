pipeline {
  agent {
    label 'Jenkins-slave'
  }
  stages {
    stage('checkout'){
      steps{
        checkout scm
      }
    }
    stage('Build'){
      steps{
        sh "mvn clean  install"
      }
    }
    stage('Depoly')"{
      steps{
        sshagent(['maven-key']) {
          sh "scp -0 StrictHostKeyChecking=no /target/my-app-1-RELEASE.jar shiva@172.31.85.15:/home/shiva"
        }
      }
    }
  }
}

